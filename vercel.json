{
  "version": 2,
  "name": "vendor-connect",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "app/api/sms/route.ts": {
      "maxDuration": 30
    }
  }
} 